class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int size= nums.size();
        int low = 0, right = size-1;
        int ans;
        while(low<=right){
            int mid = low + (right-low)/2;
            if(nums[mid]== target){
                return mid;
            }
            if(nums[mid]<target){
                low = mid+1;
            }
            else{
                right = mid-1;
            }
        }
        return low;
    }
};
